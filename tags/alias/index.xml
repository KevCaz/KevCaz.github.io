&lt;td style="vertical-align:top;padding:0;margin:0;border:0;;width:100%">
&lt;pre tabindex="0" style="color:#575279;background-color:#faf4ed;-moz-tab-size:4;-o-tab-size:4;tab-size:4;">&lt;code class="language-sh" data-lang="sh">&lt;span style="display:flex;">&lt;span>$ &lt;span style="color:#d7827e">alias&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">g&lt;/span>&lt;span style="color:#797593">=&lt;/span>git
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">gCO&lt;/span>&lt;span style="color:#797593">=&lt;/span>&lt;span style="color:#ea9d34">&amp;#39;gCo $(gCl)&amp;#39;&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">gCT&lt;/span>&lt;span style="color:#797593">=&lt;/span>&lt;span style="color:#ea9d34">&amp;#39;gCt $(gCl)&amp;#39;&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">gCa&lt;/span>&lt;span style="color:#797593">=&lt;/span>&lt;span style="color:#ea9d34">&amp;#39;git add $(gCl)&amp;#39;&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">gCe&lt;/span>&lt;span style="color:#797593">=&lt;/span>&lt;span style="color:#ea9d34">&amp;#39;git mergetool $(gCl)&amp;#39;&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">gCl&lt;/span>&lt;span style="color:#797593">=&lt;/span>&lt;span style="color:#ea9d34">&amp;#39;git --no-pager diff --name-only --diff-filter=U&amp;#39;&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">gCo&lt;/span>&lt;span style="color:#797593">=&lt;/span>&lt;span style="color:#ea9d34">&amp;#39;git checkout --ours --&amp;#39;&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">gCt&lt;/span>&lt;span style="color:#797593">=&lt;/span>&lt;span style="color:#ea9d34">&amp;#39;git checkout --theirs --&amp;#39;&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#797593">[&lt;/span>...&lt;span style="color:#797593">]&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">gws&lt;/span>&lt;span style="color:#797593">=&lt;/span>&lt;span style="color:#ea9d34">&amp;#39;git status --ignore-submodules=${_git_status_ignore_submodules} --short&amp;#39;&lt;/span>
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>&lt;span style="color:#d7827e">gwx&lt;/span>&lt;span style="color:#797593">=&lt;/span>&lt;span style="color:#ea9d34">&amp;#39;git rm -r&amp;#39;&lt;/span>
&lt;/span>&lt;/span>&lt;/code>&lt;/pre>&lt;/td>&lt;/tr>&lt;/table>
